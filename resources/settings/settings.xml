            <listEntry value="Pacific/Honolulu">@Strings.ZoneHonolulu</listEntry>
            <listEntry value="Africa/Johannesburg">@Strings.ZoneJohannesburg</listEntry>
        </settingConfig>
    </setting>
    <setting propertyKey="@Properties.City1Label" title="@Strings.City1LabelTitle">
